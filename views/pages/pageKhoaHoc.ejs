        <h2>DANH SÁCH GIẢNG VIÊN</h2>
        <h6>*** Chọn giảng viên để học ***</h6>
        <div class="team-leader-block clearfix">
        <% data.forEach(function(val,key){ %>
            <div class="team-leader-box">
                <div class="team-leader wow fadeInDown delay-03s"> 
                    <div class="team-leader-shadow"><a href="/khoa-hoc/<%=val.Aslias_KH%>/<%=val.Aslias_NV%>/<%=val.idNV%>/<%=val.idKhoaHoc%>"></a></div>
                    <img src="/images/<%=val.HinhNV%>" alt="">
                    <ul>
                        <li><a href="" class="fa-twitter"></a></li>
                        <li><a href="" class="fa-facebook"></a></li>
                        <li><a href="" class="fa-pinterest"></a></li>
                        <li><a href="" class="fa-google-plus"></a></li>
                    </ul>
                </div>
                <h3 class="wow fadeInDown delay-03s"><%=val.HoTen%></h3>
                <span class="wow fadeInDown delay-03s"><%=val.Email%></span>
                <p class="wow fadeInDown delay-03s"> <%=val.GT%></p>
            </div>
        <%})%>
        </div>
    </div>
</section><!--main-section team-end-->
